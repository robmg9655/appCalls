{
  "expo": {
    "name": "AppCalls",
    "slug": "AppCalls",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logocalls.png",
    "scheme": "restate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logocalls.png",
        "backgroundColor": "#000000"
      },
      "package": "com.robgm.AppCalls"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logocalls.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logocalls.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/NimbusSanL-Bol.ttf",
            "./assets/fonts/NimbusSanL-Reg.ttf",
            "./assets/fonts/NimbusSanL-RegIta.ttf",
            "./assets/fonts/NimbusSanL-BolIta.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
